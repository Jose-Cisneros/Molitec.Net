@model IEnumerable<molitec.Data.Models.CartaDePorte>

    @{
    ViewData["Title"] = "Cartas de Porte";
    }

    <h2>Cartas de Porte </h2> 

    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.FechaEmi)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.FechaVenci)
                </th>

                <th>
                    @Html.DisplayNameFor(model => model.Numero)
                </th>

                <th>
                    @Html.DisplayNameFor(model => model.Cee)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Ctg)
                </th>

                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model) {
            <tr>
                
                <td>
                    @Html.DisplayFor(modelItem => item.FechaEmi)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FechaVenci)
                </td>
               
                <td>
                    @Html.DisplayFor(modelItem => item.Numero)
                </td>
                
                <td>
                    @Html.DisplayFor(modelItem => item.Cee)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Ctg)
                </td>
                
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Editar</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Detalles</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Eliminar</a>
                </td>
            </tr>
            }
        </tbody>
    </table>
<div>
    <a  asp-controller="contratoCompras" asp-action="index" >Volver</a> 
</div>
